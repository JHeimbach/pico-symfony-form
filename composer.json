{
  "name": "jheimbach/pico-symfony-form",
  "type": "pico-plugin",
  "description": "A Content variables plugin for Pico CMS.",
  "keywords": [ "pico", "picocms", "picocms-plugin", "pico-symfony-form", "pico-sym-form", "PicoSymForm"],
  "homepage": "http://picocms.org/",
  "license": "MIT",
  "authors": [
    {
      "name": "Johannes Heimbach",
      "homepage": "https://github.com/jheimbach/pico-symfony-form",
      "role": "Lead Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/jheimbach/pico-symfony-form/graphs/contributors"
    }
  ],
  "support": {
    "docs": "https://github.com/jheimbach/pico-symfony-form/blob/master/README.md",
